<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="icon" href="path/to/your/icon.ico" type="image/x-icon">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Josefin+Sans:ital,wght@0,100..700;1,100..700&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <script src="https://kit.fontawesome.com/0d8e1c9927.js" crossorigin="anonymous"></script>
    <style>
        body{
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            overflow: hidden;
            background-color: #192043;
        }
        .container{
          
            width: 100vw;
            overflow: hidden;
            font-family: "Poppins";
            padding: 15;
        }
        header{
           
            border: none;
            display: flex;
          padding: 9;
          transform: translateX(19%);
          transition: all .9s linear;
          width: 80%;
          background: linear-gradient(to right, #2e4092, #113cd7);
          border-radius: 0px 10px 0px 0px;
          
        }
        .top-header{
            width: 95%;
          
            margin-left: 10;
            height: 40px;
           

        }
        .account-profile{
            position: relative;
            top: -50;
            display: flex;
          
           float: right;
         
            
            justify-content: center;
           
        }
        .img-holder{
            position: relative;
            top: 5;
            width: 50px;
            height: 50px;
            border-radius: 50%;
        }
        img{
            width: 100%;
            height: 100%;
            border-radius: 50%;
        }
        .profile-name{
            margin-left: 10;
            text-align: center;
            color: white;
            font-size: 15px;
        }
        .toogle-menu i{
            font-size: 20px;
        }
        .side-menu{
            position: absolute;
            top: 15;
        background-color: #2a2a2b;
            width: 226px;
            height: 70vh;
            transition: all .9s linear;
            border-radius: 10px 0px 0px 10px;
        }
        .side-menu .header{
            position: relative;
           height: 58px;
           top: -16;
         border-radius: 10px 0px 0px 0px;
        
            text-align: center;
            font-family: inherit;
            background: linear-gradient(to right, #041a7c, #2f56e5);
            
        }
        .logo{
            width: 100%;
            height: 100%;
            color: white;
            position: relative;
            top: 16;
        }
        .main{
           
            transition: all .9s linear;
          padding: 18;
          transform: translateX(225px);
          width: 1152px;
          height: 58vh;
          background-color: #e4e4e4;
        overflow-y: auto;
        
        
        }
        .toogle-menu button{
padding: 10;
border: none;
background-color: transparent;
cursor: pointer;
transition: all .5s linear;
color: white;
        }
        .toogle-menu button:hover{
            background-color: #041a7c;
        }
       .mail-box i{
       
        padding: 15;
        margin-right: 40;
font-size: 25px;
color: #2380c8;
        }
        .mail-box {
            position: relative
        }
        .mail-box span{
            position: absolute;
            right: 45;
            top: 6;
            border-radius: 50%;
          
            padding: 5;
            font-size: 8px;
            background-color: rgb(188, 50, 50);
            color: white;
            font-family: inherit;
            text-align: center;
        }
        h5,h2{
display: inline-block;
margin: 5;
        }
        .main h5{
            color: #57585b;
        }
        .card{
            border: 1px solid #4560be;
            width: 300px;
            background-color: #4560be;
            color: white;
            border-radius: 5%;
            margin: 15px;
        }
        .details{
            text-align: center;
           
            width: 150px;
         
        }
        .footer{
         
            text-align: center;
            background-color: #4560be;
           
        }
        .icon-holder{
            width: 150px;
          
            text-align: center;
        }
        .icon-holder i{
            transform: translateY(15px);
            padding: 25;
            padding-left: 65;
            font-size: 60px;
            color: rgb(196, 200, 196,0.6)
         
        }
        .card-h{
            display: flex;
        }
        .footer i{
            color:white;
            
        }
        .num p{
            font-size: 25px;
        }
        .card-section{
            display: flex;
        }
        .menu-item-first{
           
            margin: 10px;
            display: flex;
            color: white;
            text-align: center;
          
            justify-content: space-evenly;
            margin-top: 40px;
            
        }
        .menu-item{
           
           margin: 10px;
           display: flex;
           color: white;
           text-align: center;
         
           justify-content: space-evenly;
     
           
       }
       .icon-menu i{
        transform: translateY(5px);
       }
       .icon-menu-cart i{
        transform: translateY(8px);
       }
       
        .icon-menu{
           
           
            width: 60px;
           
        }
        .icon-menu-cart{
        
         
            width: 60px;
            
        }
        .menu-txt{
         
            width: 100px;
        }
        .menu-txt p{
            text-align: start;
        }
        .search{
            display: inline-flex;
         position: relative;
        
         left: 400px;
            font-family: inherit;
            font-size: 15px;

        }
        .search p{
            margin: 10;
        }
        .search input{
           width: 400px;
           padding: 10px;
        }
        td , th {
            padding: 10px;
            text-align: center;
        }
        th{
            font-family: "Josefin Sans", sans-serif;;
        }
        td{
         
          
           
        }
        table{
            padding: 5;
        
        }
        .menu-item:hover{
background-color: rgb(224, 221, 221);
color: #0e2165;
        }
 
       
      
       
    </style>
</head>
<body>
    <div class="container">
                <header>
   
         <div class="top-header">
        <div class="toogle-menu"><button><i class="fa-solid fa-bars" onclick="toggleMenu()"></i></button></div>
        <div class="account-profile"><div class="mail-box"><span>5+</span><i class="fa-sharp fa-solid fa-envelope" ></i></div><div class="img-holder"><img src="./images.jpeg" alt=""></div><div class="profile-name"><p>Mohhamed Hhjsu</p></div></div>
            <div class="account-display"></div>
    </div>
                </header>

<div class="side-menu">
<div class="header">
    <div class="logo">
        <p></p>
    </div>
    <div class="menu-item-first">
       <div class="icon-menu"> <p><i class="fa-solid fa-chart-line"></i></p></div> <div class="menu-txt"><p> Overview</p></div> 
    </div>
    <div class="menu-item">
        <div class="icon-menu-cart"> <p><i class="fa-solid fa-cart-plus"></i></p></div><div class="menu-txt"><p> Orders</p></div>
    </div>
    <div class="menu-item">
        <div class="icon-menu">  <p><i class="fa-solid fa-users"></i></p></div> <div class="menu-txt"><p> Customers</p></div>
    </div>

    <div class="menu-item">
        <div class="icon-menu">  <p><i class="fa-solid fa-cart-arrow-down"></i></p></div> <div class="menu-txt"><p> Materials</p></div>
      
    </div>
    <div class="menu-item">
        <div class="icon-menu">  <p><i class="fa-solid fa-house"></i></p></div> <div class="menu-txt"><p>Website</p></div>
      
    </div>
   
</div>
</div>

<div class="main">


    <h2>Color</h2><div class="search"><p>Search For Customer</p><input type="text" placeholder="Id Number"></div>
   
<table>
    <th>Name</th>
    <th>id</th>
    
    <tbody>
       
      
            <% color.forEach(cust => { %>
                <tr>
                  
                    <td><%= cust.name %></td>
                    <td><%= cust.id %></td>
                </tr>
            <% }) %>

        
           
  
        
    </tbody>

</table>
</div>

    </div>
    
    <script>
let clicked =false
function toggleMenu(){
 clicked =!clicked
 if(clicked){
    
    const Menutxt = document.querySelectorAll('.menu-txt')
    const sideM = document.querySelector('.side-menu')
    const header = document.querySelector('header')
    const main = document.querySelector('.main')
    sideM.style.width='121px'
    header.style.transform = 'translateX(9%)';
   
    main.style.transform = 'translateX(8%)';
    header.style.width = '83%';
    main.style.width = '83%';
    setTimeout(()=>{
Menutxt.forEach(node =>{
    node.style.display="none"
})
    },800)
 }
 else{
    const Menutxt = document.querySelectorAll('.menu-txt')
    const sideM = document.querySelector('.side-menu')
    const header = document.querySelector('header')
    const main = document.querySelector('.main')
    sideM.style.width='226px'
    header.style.transform = ' translateX(19%)';
    main.style.transform = ' translateX(225px)';
    header.style.width = '80%';
    main.style.width = '1152px';
    setTimeout(()=>{
Menutxt.forEach(node =>{
    node.style.display="block"
})
    },900)
 }
console.log(clicked)
}

    </script>
</body>
</html>